<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.Dao.Personal.PersonalLogin">
  <select id="Login" resultType="String" parameterType="User">
    SELECT Membersid from family where Familyname=#{familyname} and Familypassword=#{familypassword}
  </select>
</mapper>
